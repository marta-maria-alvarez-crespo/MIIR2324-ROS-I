#!/usr/bin/env python
import rospy 
from practica_03 import service

def inicia_nodo():
    try:
        rospy.init_node("service") 
        service.servicio() 
    except rospy.ROSInterruptException:
        rospy.logerr("O presionaste CTRL+C o algo sali√≥ mal...")

if __name__ == "__main__":
    inicia_nodo()